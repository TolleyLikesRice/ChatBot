{
  "name": "chatbot",
  "version": "2.0.0",
  "description": "W.I.P All-in-One Discord Bot",
  "main": "app.js",
  "scripts": {
    "test": "mocha --reporter spec",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "devcover": "tsc ./ts/maindefs ./ts/fbdefs --outDir ./ && istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "tsc": "tsc ./ts/maindefs ./ts/fbdefs --outDir ./"
  },
  "repository": "https://github.com/TolleyB-J/ChatBot",
  "author": "Tolley-BJ",
  "license": "MIT",
  "dependencies": {
    "boxen": "^1.3.0",
    "capture-console": "^1.0.1",
    "chalk": "^2.4.1",
    "discord.js": "^11.3.2",
    "firebase-admin": "^5.12.1",
    "giphy-js-sdk-core": "^1.0.3",
    "moment": "^2.22.1",
    "money": "^0.2.0",
    "ms": "^2.1.1",
    "nedb": "^1.8.0",
    "random-number": "0.0.9",
    "request": "^2.87.0",
    "sync-request": "^6.0.0",
    "toml": "^2.3.3",
    "uws": "^10.148.0",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
